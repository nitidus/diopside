<!-- Libraries-->
<% if (typeof sub != 'undefined') { %>
  <% if (sub == 'service/tokenized/scheduled/media') { %>
    <script src="<%= base %>vendors/js/scheduled-media-min.js"></script>
  <% }else if (sub == 'service/tokenized/scheduled/media/upload') { %>
    <script src="<%= base %>vendors/js/media-upload-min.js"></script>
  <% }else if (sub == 'service/tokenized') { %>
    <script src="<%= base %>vendors/js/advanced-chart-min.js"></script>
  <% }else if (sub == 'dashboard'){ %>
    <script src="<%= base %>vendors/js/starter-chart-min.js"></script>
  <% }else if (sub == 'new-account' || sub == 'authorized') { %>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCQQwxmPEZPNchzrARjmY5RRpr5gzVqSWA"></script>
    <script src="<%= base %>vendors/js/numb-min.js"></script>
  <% }else if (sub == 'credit') { %>
    <script src="<%= base %>vendors/js/numb-min.js"></script>
  <% }else{ %>
    <script src="<%= base %>vendors/js/regular-min.js"></script>
  <% } %>
<% }else{ %>
  <script src="<%= base %>vendors/js/regular-min.js"></script>
<% } %>
